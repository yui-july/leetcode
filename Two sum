class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        l=len(nums)
        for i in range(l):
            for j in range(i+1,l):
                if nums[i]+nums[j]==target:
                    return i,j

class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        hashmap = {} ##哈希表
        for i, value in enumerate(nums):
            ## diff = target-value
            if target-value in hashmap:
                return [hashmap[target-value], i]
            hashmap[value] = i
